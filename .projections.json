{
	"app/models/*.rb": {
	    "type": "model",
	    "alternate": "spec/models/{}_spec.rb"
	},
	"spec/models/*_spec.rb": {
	    "type": "modelTest",
	    "alternate": "app/models/{}.rb"
	},
	"app/controllers/*.rb": {
    "type": "controller",
    "alternate": "spec/controllers/{}_spec.rb"
  },
	"spec/controllers/*_spec.rb": {
	    "type": "controllerTest",
	    "alternate": "app/controllers/{}.rb"
	},

	"app/sidekiq/*.rb": {
    "type": "job",
    "alternate": "spec/sidekiq/{}_spec.rb"
  },
	"spec/sidekiq/*_spec.rb": {
	    "type": "jobTest",
	    "alternate": "app/sidekiq/{}.rb"
	},

	"app/helpers/*_helper.rb": { "type": "helper" }
}
